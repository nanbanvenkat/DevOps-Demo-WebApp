pipeline {
         agent any
	 stages {
                 stage('Clone source') {
                 steps {
                      git url: 'https://github.com/nanbanvenkat/DevOps-Demo-WebApp.git'
                 }
                 }
                
                 stage('Deploy to test') {
                 steps {
                  deploy adapters: [tomcat8(url: 'http://52.167.166.3:8080/', credentialsId: 'tomcat', path: '' )], contextPath: '/QAWebapp', war: '**/*.war'
	            }
                 }
               
              }
}
